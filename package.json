{
  "name": "mega-chat",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "prisma": "^4.2.1",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1"
  },
  "packageManager": "yarn@1.22.19"
}